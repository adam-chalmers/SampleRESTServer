{
    "extends": "xo-space",
    "rules": {
      "one-var": 0,
      "space-before-function-paren": [2, { "anonymous": "always", "named": "always" }],
      "padded-blocks": 0,
      "space-before-blocks": 0,
      "camelcase": 0,
      "handle-callback-err": 0,
      "no-mixed-requires": 0,
      "indent": 0,
      "max-params": 0,
      "max-lines": 0
    }
}
